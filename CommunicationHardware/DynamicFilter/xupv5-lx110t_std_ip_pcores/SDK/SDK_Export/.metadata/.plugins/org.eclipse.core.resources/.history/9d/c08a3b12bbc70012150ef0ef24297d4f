# Tool invocations
Virtex5Dvi.elf: $(OBJS) ../src/lscript.ld $(USER_OBJS)
	@echo Building target: $@
	@echo Invoking: MicroBlaze gcc linker
	mb-gcc -Wl,--no-relax -Wl,-T -Wl,../src/lscript.ld -L../../standalone_bsp_0/microblaze_0/lib -mxl-pattern-compare -mcpu=v7.30.b -mno-xl-soft-mul -o"Virtex5Dvi.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo Finished building target: $@
	@echo ' '

Virtex5Dvi.elf.size: Virtex5Dvi.elf
	@echo Invoking: MicroBlaze Print Size
	mb-size Virtex5Dvi.elf  |tee "Virtex5Dvi.elf.size"
	@echo Finished building: $@
	@echo ' '

Virtex5Dvi.elf.elfcheck: Virtex5Dvi.elf
	@echo Invoking: Xilinx ELF Check
	elfcheck Virtex5Dvi.elf -hw ../../xupv5-lx110t_std_ip_pcores_hw_platform/system.xml -pe microblaze_0  |tee "Virtex5Dvi.elf.elfcheck"
	@echo Finished building: $@
	@echo ' '

